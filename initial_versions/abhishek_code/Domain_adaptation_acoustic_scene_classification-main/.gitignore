*.py[cod]
*$py.class
.pytest_cache/
.tox/
.nox/
.mypy_cache/
.pyre/
.pytype/
*.egg-info/
.eggs/
build/
dist/
eggs/

# Virtual environments
venv/
.env/
.envs/
.venv/
env/
ENV/

# Jupyter / IPython
.ipynb_checkpoints/
profile_default/
ipython_config.py

# Editor/IDE
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# OS files
.DS_Store
Thumbs.db
desktop.ini
._*

# Temporary files
tmp/
temp/
*.tmp
*.log

# Databases
*.sqlite
*.sqlite3
*.db

# Node / frontend
node_modules/

# Coverage / test reports
.coverage
coverage.xml
nosetests.xml
htmlcov/

# Common archives and large media (exclude large binary/media files)
*.zip
*.tar
*.tar.gz
*.tgz
*.gz
*.rar
*.7z
